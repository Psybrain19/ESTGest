@model IEnumerable<ESTGest.Models.ChargeAccountReference>

@{
    ViewBag.Title = "ReferenceList";
}

<h2>Lista de Referências</h2>

<p>
    @Html.ActionLink("Adicionar", "ReferenceCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.car_reference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.car_state)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.car_valtime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.car_amount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.car_reference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.car_state)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.car_valtime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.car_amount)
        </td>
        <td>
            @Html.ActionLink("Editar", "ReferenceEdit", new { id=item.car_id }) |
            @Html.ActionLink("Detalhes", "ReferenceDetails", new { id=item.car_id }) |
            @Html.ActionLink("Apagar", "ReferenceDelete", new { id=item.car_id })
        </td>
    </tr>
}

</table>
